{
  "name": "bikeindex",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && PORT=8080 npx serve",
    "start": "webpack serve --open --mode development",
    "start:live": "webpack serve --open --mode development --live-reload --hot",
    "test": "jest --watchAll=false --coverage"
  },
  "license": "MIT",
  "author": {
    "name": "bahamon94",
    "email": "bahamon756@hotmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.5.1",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.0.2",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/.{js,jsx}",
      "!src/serviceWorker.ts",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 10,
        "functions": 10,
        "lines": 10,
        "statements": 10
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "html"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "axios": "^0.26.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-leaflet": "^3.2.5",
    "wouter": "^2.8.0-alpha.2"
  }
}
